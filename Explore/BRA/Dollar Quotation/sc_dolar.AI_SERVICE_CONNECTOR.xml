            <description>Service connector reponsavel pelos acessos aos metodos da api de DOLAR do Governo.
https://olinda.bcb.gov.br</description>
            <tags/>
            <connectionAttributes xmlns="http://schemas.informatica.com/appmodules/screenflow/2014/04/avosConnectors.xsd">
               <connectionAttribute description="Url de Origem do Serviço"
                                    masked="false"
                                    name="url"
                                    required="true"
                                    testWith="https://olinda.bcb.gov.br"
                                    type="string"/>
            </connectionAttributes>
            <actions>
               <action category="GET"
                       failOnError="true"
                       forSearch="false"
                       label="Get Dolar Periodo"
                       maxRedirects="100"
                       name="Get Dolar Periodo"
                       preemptiveAuth="true"
                       responseEncoding="">
                  <description>Metodo que retorna a Cotação Dolar em Periodo</description>
                  <input>
                     <parameter description="Data de inicio da Consulta"
                                name="Data_Inicio"
                                nogen="true"
                                required="true"
                                testWith="01-01-2021"
                                type="string"/>
                     <parameter description="Data de Fim da Consulta"
                                name="Data_Fim"
                                nogen="true"
                                required="true"
                                testWith="09-01-2021"
                                type="string"/>
                  </input>
                  <binding>
                     <restSimpleBinding multiUsing="semicolonSeparated"
                                        url="{$url}/olinda/servico/PTAX/versao/v1/odata/CotacaoDolarPeriodo(dataInicial=@dataInicial,dataFinalCotacao=@dataFinalCotacao)?@dataInicial='{$Data_Inicio}'&amp;amp;@dataFinalCotacao='{$Data_Fim}'&amp;amp;$top=100&amp;amp;$format=json&amp;amp;$select=cotacaoCompra,cotacaoVenda,dataHoraCotacao"
                                        verb="GET"/>
                  </binding>
                  <output>
                     <field description="Return da cotação de compra e venda do dolar em determinado periodo."
                            name="return_get_cotacao_periodo"
                            type="reference"
                            xmlElement=".">
                        <options>
                           <option name="referenceTo">root</option>
                        </options>
                     </field>
                  </output>
               </action>
               <action category="GET"
                       failOnError="true"
                       forSearch="false"
                       label="Get Dolar Dia"
                       maxRedirects="100"
                       name="Get Dolar Dia"
                       preemptiveAuth="true"
                       responseEncoding="">
                  <description>Método que retorna a cotação do dólar de um dia específico</description>
                  <input>
                     <parameter description="Data da consulta"
                                name="DataCotacao"
                                nogen="true"
                                required="true"
                                testWith="09-16-2021"
                                type="string"/>
                  </input>
                  <binding>
                     <restSimpleBinding multiUsing="semicolonSeparated"
                                        url="{$url}/olinda/servico/PTAX/versao/v1/odata/CotacaoDolarDia(dataCotacao=@dataCotacao)?@dataCotacao='{$DataCotacao}'&amp;amp;$top=100&amp;amp;$format=json&amp;amp;$select=cotacaoCompra,cotacaoVenda,dataHoraCotacao"
                                        verb="GET"/>
                  </binding>
                  <output>
                     <field description="Retorno da cotação do dólar de um dia específico"
                            name="return_get_cotacao_dia"
                            type="reference"
                            xmlElement=".">
                        <options>
                           <option name="referenceTo">root</option>
                        </options>
